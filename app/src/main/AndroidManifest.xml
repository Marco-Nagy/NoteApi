<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notesapi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_notebook"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_notebook"
        android:supportsRtl="true"
        android:theme="@style/Theme.NotesApi">
        <activity android:name=".ui.EditNote"></activity>
        <activity android:name=".ui.AddNoteActivity"></activity>
        <activity android:name=".ui.RegisterMainActivity" />
        <activity
            android:name=".ui.NotesMainActivity"
            android:label="Main Notes" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>